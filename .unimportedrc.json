{
  "entry": ["src/main.tsx"],
  "extensions": [".ts", ".tsx", ".js", ".jsx"],
  "ignorePatterns": [
    "**/node_modules/**",
    "**/dist/**",
    "**/coverage/**",
    "**/test-results/**",
    "**/playwright-report/**",
    "**/*.test.*",
    "**/*.spec.*",
    "**/types.ts",
    "**/index.ts"
  ],
  "ignoreUnresolved": [],
  "ignoreUnimported": ["src/test/**", "src/types/**"],
  "aliases": {
    "@": "src"
  }
}
