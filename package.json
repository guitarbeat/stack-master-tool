{
  "name": "stack-master-tool",
  "private": true,
  "version": "1.0.0",
  "description": "Meeting facilitation application with real-time speaking queue management",
  "type": "module",
  "author": "Stack Master Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite --config config/vite.config.ts",
    "build": "vite build --config config/vite.config.ts",
    "build:dev": "vite build --mode development --config config/vite.config.ts",
    "build:prod": "vite build --mode production --config config/vite.config.ts",
    "build:analyze": "vite build --mode production --reporter verbose --config config/vite.config.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview --config config/vite.config.ts",
    "preview:prod": "vite preview --host 0.0.0.0 --config config/vite.config.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest run --coverage --ui",
    "setup-dev": "node scripts/setup-dev-meetings.js",
    "test:coverage:watch": "vitest --coverage --watch",
    "test:watch": "vitest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:run && npm run test:e2e",
    "test:full": "npm run test:run && npm run test:e2e",
    "test:full:coverage": "npm run test:coverage",
    "test:ci": "npm run test:run",
    "test:comprehensive": "node scripts/test-comprehensive.js",
    "test:performance": "vitest run --config vitest.config.performance.ts",
    "test:unit:watch": "vitest --watch",
    "test:unit:ui": "vitest --ui",
    "coverage:report": "node scripts/coverage-report.js",
    "test:dev": "node scripts/test-runner.js",
    "test:dev:watch": "node scripts/test-runner.js watch",
    "test:dev:coverage": "node scripts/test-runner.js coverage",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .build/dist node_modules/.vite coverage .reports/test-results .reports/playwright-report *.log",
    "cap:sync": "cap sync",
    "cap:build:android": "cap build android",
    "cap:build:ios": "cap build ios",
    "deploy:netlify": "npm run build:prod && netlify deploy --prod --dir ./.build/dist",
    "deploy:vercel": "npm run build:prod && vercel --prod",
    "security:audit": "npm audit --audit-level moderate",
    "bundle:analyze": "npm run build:analyze"
  },
  "dependencies": {
    "@capacitor/core": "^7.4.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/supabase-js": "^2.74.0",
    "@tanstack/react-query": "^5.83.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.462.0",
    "next-themes": "^0.3.0",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "sonner": "^1.7.4",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@dyad-sh/react-vite-component-tagger": "^0.8.0",
    "@eslint/js": "^9.32.0",
    "@playwright/test": "^1.49.1",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.16.5",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "@vitest/coverage-v8": "^2.1.8",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "^16.2.3",
    "lovable-tagger": "^1.1.9",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "sharp": "^0.34.3",
    "tailwindcss": "^3.4.17",
    "terser": "^5.44.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "unimported": "^1.31.0",
    "vite": "^5.4.19",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
